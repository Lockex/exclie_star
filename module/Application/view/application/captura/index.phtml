	<script>
	$(document).ready(function() {
		$('#capturando').hide();
		$('#conyuge').hide();
		



		$("#fecha_consulta").on('change',function () {
	        var valorfec = $('#fecha_consulta').val();
	        $("#fecha_consulta2").val(valorfec);
    	
    	});
    
   	});
	var _ruta = '<?php echo $this->basePath(); ?>';	
	
	$('#rootwizard1 .finish').click(function() {
			
		var formulario = $('#formPac').serialize();
		
			$.ajax({
			    type: "POST",
			    url: "<?php echo $this->basePath() ?>/captura/guardarpaciente",
			    dataType: "json",
			    data: formulario,
			    success: function(respuesta) {
			       $("#verpacientes").hide();
			       $('#capturando').show();
			       idPaciente = respuesta.id;
			       nombrepac = respuesta.nombre+' '+respuesta.apellido;		       
				   $('#idPaciente').val(idPaciente);
				   $('#nombrePaciente').text(nombrepac);

			    },
			    error: function(){
			          alert('Ocurrió un error, inténtelo más tarde.');
			    }
			});
		
		
		
	});

	function nuevoExpediente(){
		 $("#confirma").modal('show');
		 $('#confirma').modal({ backdrop: 'static', keyboard: false })
		 .one('click', '#nuevoExp', function (e) {
		 	$("#confirma").modal('hide');
		 	window.location.href = _ruta+'/captura';
		});
	}

	function ocultarCoyuge () {
		if($("#sEstadocivil").val() == 1)
		$("#conyuge").hide();
		else
		$("#conyuge").show();	
	}

	Dropzone.autoDiscover = false;

	var myDropzone = new Dropzone("#dropa", {
		paramName: "file", // The name that will be used to transfer the file
		  accept: function(file, done) {
		    done();
		  },
		  addRemoveLinks: true,
		  removedfile: function(file){
		    var datos = {archivo:file.name,id:$('#idPaciente').val()};
		    console.log(datos);
		    $.ajax({
		          type: "POST",
		          url: "<?php echo $this->basePath() ?>/captura/eliminarFoto",
		          data: datos,
		          dataType: "json",
		          success: function(data) {
		                  var _ref;
		                     if (file.previewElement) {
		                       if ((_ref = file.previewElement) != null) {
		                         _ref.parentNode.removeChild(file.previewElement);
		                       }
		                     }
		                     return this._updateMaxFilesReachedClass();
		                },
		          error: function(data){
		                      var _ref;
		                        if (file.previewElement) {
		                          if ((_ref = file.previewElement) != null) {
		                            _ref.parentNode.removeChild(file.previewElement);
		                          }
		                        }
		                        return this._updateMaxFilesReachedClass();

		          }
		      });
		  }
	});

	

	</script>